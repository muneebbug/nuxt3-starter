{
  "name": "nuxt-starter",
  "version": "3.0.1",
  "private": true,
  "repository": "https://github.com/muneebbug/nuxt3-starter",
  "homepage": "https://nuxt3-starter-bay.vercel.app",
  "scripts": {
    "build": "prisma generate && nuxt build",
    "dev": "nuxt dev",
    "generate": "prisma generate && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "release": "npm run lint && npm run typecheck && changelogen --release && git push --follow-tags",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "prisma:generate": "npx prisma generate",
    "prisma:db:push": "npx prisma db push",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "pnpm prisma:db:push --force-reset && npx prisma db seed",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio"
  },
  "devDependencies": {
    "@bg-dev/nuxt-auth": "3.0.1",
    "@bg-dev/nuxt-naiveui": "2.0.0-rc.1",
    "@bg-dev/nuxt-s3": "^2.0.1",
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/icon": "^1.5.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/ua-parser-js": "^0.7.39",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.12.2",
    "nuxt-security": "^1.2.2",
    "prisma": "^5.12.0",
    "shadcn-nuxt": "^0.10.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "5.12.0",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.439.0",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "ua-parser-js": "^1.0.37",
    "vee-validate": "^4.13.2",
    "zod": "^3.23.8"
  }
}
